clear all
close all
%[x,y] = textread('out_cut.txt','%n %n', 'headerlines',2);

fid = fopen();
input = textscan('out_cut.txt','%d %d', 65, 'headerlines',2);
x = input(:,1);
y = input(:,2);

x_start = 2;
y_start = 3;

x_end = x(64);
y_end = y(64);

show_ind = 1:64;
plot(x(show_ind),y(show_ind),'r*-', ...
    x_start, y_start,'bo',...
    x_end, y_end,'bo')
grid on




